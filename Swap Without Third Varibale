import java.util.*;

public class Swap {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        System.out.println("Enter N=");
        int x = s.nextInt();
        int y = s.nextInt();
        System.out.println(x + " " + y);
        x = x ^ y;
        y = x ^ y;
        x = x ^ y;
        System.out.println(x + " " + y);
    }
}
